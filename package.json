{
	"name": "@master-key-g/gulp-shell",
	"version": "1.11.0",
	"description": "A handy command line interface for gulp",
	"keywords": [
		"gulpplugin",
		"gulp",
		"shell",
		"command"
	],
	"homepage": "https://github.com/master-key-g/gulp-shell",
	"bugs": {
		"url": "https://github.com/master-key-g/gulp-shell/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/master-key-g/gulp-shell.git"
	},
	"license": "MIT",
	"author": "master861",
	"type": "commonjs",
	"main": "lib/index.js",
	"types": "lib/index.d.ts",
	"directories": {
		"lib": "lib"
	},
	"files": [
		"lib/index.d.ts",
		"lib/index.d.ts.map",
		"lib/index.js",
		"lib/index.js.map",
		"README.md"
	],
	"publishConfig": {
		"access": "public"
	},
	"mocha": {
		"spec": [
			"*.test.ts"
		],
		"require": [
			"ts-node/register"
		]
	},
	"scripts": {
		"build": "gulp build",
		"test": "nyc --reporter=html --reporter=text-summary --reporter=lcov mocha --require mocha-suppress-logs",
		"lint": "eslint --fix .",
		"watch": "gulp watch",
		"coverage": "nyc npm run test",
		"coveralls": "gulp coveralls",
		"prepare": "npm run build",
		"prepublishOnly": "npm run prepare"
	},
	"dependencies": {
		"acorn": "^8.15.0",
		"ajv": "^8.17.1",
		"chalk": "^5.6.2",
		"copy-props": "^4.0.0",
		"decode-uri-component": "^0.4.1",
		"expand-template": "^2.0.3",
		"fancy-log": "^2.0.0",
		"json5": "2.2.3",
		"path-parse": "1.0.7",
		"plugin-error": "^2.0.1",
		"qs": "^6.14.0",
		"through2": "^4.0.2",
		"tmpl": "1.0.5",
		"ts-node-register": "^1.0.0",
		"ws": "^8.18.3",
		"y18n": "^5.0.8"
	},
	"devDependencies": {
		"@babel/preset-env": "^7.28.5",
		"@babel/preset-typescript": "^7.28.5",
		"@eslint/js": "^9.39.1",
		"@hapi/code": "^9.0.3",
		"@istanbuljs/nyc-config-typescript": "^1.0.2",
		"@types/ejs": "^3.1.5",
		"@types/eslint-plugin-mocha": "^10.4.0",
		"@types/fancy-log": "^2.0.2",
		"@types/gulp": "^4.0.18",
		"@types/mocha": "^10.0.10",
		"@types/node": "^24.9.2",
		"@types/through2": "^2.0.41",
		"@typescript-eslint/eslint-plugin": "^8.46.2",
		"@typescript-eslint/parser": "^8.46.3",
		"earl": "2.0.0",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-mocha": "^11.2.0",
		"eslint-plugin-prettier": "^5.5.4",
		"globals": "^16.5.0",
		"gulp": "^5.0.1",
		"gulp-typescript": "^6.0.0-alpha.1",
		"lodash.template-async": "^0.1.9",
		"mocha": "11.7.5",
		"mocha-lcov-reporter": "^1.3.0",
		"mocha-suppress-logs": "^0.6.0",
		"nyc": "17.1.0",
		"prettier": "^3.6.2",
		"source-map-support": "^0.5.21",
		"ts-mocha": "^11.1.0",
		"ts-node": "^10.9.2",
		"ts-node-register": "^1.0.0",
		"tsconfig-paths": "^4.2.0",
		"tslib": "^2.8.1",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.46.3",
		"vinyl": "^3.0.1"
	},
	"engines": {
		"node": ">=16.0.0"
	},
	"nyc": {
		"include": [
			"**/*.ts",
			"**/*.tsx",
			"**/*.js"
		],
		"extension": [
			".ts",
			".tsx",
			".js"
		],
		"exclude": [
			"**/*.d.ts"
		],
		"require": [
			"ts-node/register"
		],
		"reporter": [
			"text-summary",
			"html",
			"lcov"
		],
		"extends": "@istanbuljs/nyc-config-typescript",
		"sourceMap": true,
		"instrument": true,
		"all": true
	}
}
